<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Single instance boot library</Description>
    <Authors>Gustavo Viana</Authors>
    <RepositoryUrl>https://github.com/gustavorviana/SingleSharpInstance</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IO.Pipes.AccessControl" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="NamedPipeServerStream.NetFrameworkVersion">
      <HintPath>NamedPipeServerStream.NetFrameworkVersion.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
